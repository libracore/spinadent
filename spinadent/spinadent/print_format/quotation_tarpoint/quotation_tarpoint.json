rder-color: black\">\n    <table style=\"width: 100%; font-family: 'Open Sans', sans-serif; padding: 0px; font-size: 9px\">\n        <tr> \n            <td style=\"width: 9.7%\"><strong>Leistungen</strong></td>\n            <td style=\"width: 13.6%\"><strong>Tarif::Code</strong></td>\n            <td style=\"width: 20.5%\"><strong>Ansatz*Faktor*Anzahl</strong></td>\n            <td style=\"width: 12.2%\"><strong>Preis</strong></td>\n            <td style=\"width: 50%\"><strong></strong></td>\n        </tr>\n        {% for item in doc.items%}\n        <tr> \n            <td><strong></strong></td>\n            <td>{{item.item_code}}</td>\n            <td>{{item.rate}}*1.00*{{item.qty}}</td>\n            <td>{{item.rate}}</td>\n            <td>{{item.item_name}}</td>\n        </tr>\n        {%endfor%}\n    </table>\n</div>\n<div style=\"height: 50px; width: 100%\"></div>\n<div style=\"display: block; border-color: black\">\n    <table style=\"width: 100%; font-family: 'Open Sans', sans-serif; padding: 0px; border: solid; border-width: thin; font-size: 9px\">\n        <tr> \n            <td style=\"width: 9.7%\"><strong>Kontakt</strong></td>\n            <td style=\"width: 13.6%\">Ansprechpartner</td>\n            <td style=\"width: 14.5%\">{{doctor.hospital}}</td>\n            <td style=\"width: 20.7%\"> </td>\n            <td style=\"width: 21.5%\"></td>\n        </tr>\n        <tr> \n            <td style=\"width: 9.7%\"><strong></strong></td>\n            <td style=\"width: 13.6%\"></td>\n            <td style=\"width: 14.5%\">Tel: {{doctor.residence_phone}}</td>\n            <td style=\"width: 20.7%\">Fax: {{doctor.residence_phone}}</td>\n            <td style=\"width: 21.5%\">eMail: </td>\n        </tr>\n        <tr> \n            <td style=\"width: 9.7%\"><strong></strong></td>\n            <td style=\"width: 13.6%\">Adresse</td>\n            <td style=\"width: 14.5%\"></td>\n            <td style=\"width: 20.7%\"> </td>\n            <td style=\"width: 21.5%\"></