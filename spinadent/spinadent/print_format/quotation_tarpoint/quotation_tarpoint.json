th: 100%; font-family: 'Open Sans', sans-serif; padding: 0px; border-left: solid; border-bottom: solid; border-width: thin; font-size: 9px\"> \n       {% set pat = frappe.get_doc(\"Patient\", doc.patient) %}\n        <tr> \n            <td style=\"width: 9.7%\"><strong>Patient</strong></td>\n            <td style=\"width: 13.6%\">Name</td>\n            <td style=\"width: 34.5%; font-size: 12px\">{{pat.patient_name.split()[0]}}</td>\n            <td style=\"width: 25.7%\"> EAN-Nr</td>\n            <td style=\"width: 16.5%\">7601001302112</td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Vorname</td>\n            <td style=\"font-size: 12px\">{{pat.patient_name.split()[1]}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Strasse</td>\n             <td style=\"font-size: 12px\">{{pat.street}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>PLZ</td>\n             <td style=\"font-size: 12px\">{{pat.pincode}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Ort</td>\n             <td style=\"font-size: 12px\">{{pat.city}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Geburtsdatum</td>\n            <td style=\"font-size: 12px\"> {{pat.dob}} </td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Geschlecht</td>\n             <td style=\"font-size: 12px\">{{pat.sex}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Unfalldatum</td>\n             <td style=\"font-size: 12px\"> {{doc.fall_unfalldatum}}</td>\n            <td></td>\n            <td></td>\n        </tr>\n        <tr> \n            <td></td>\n            <td>Unfall